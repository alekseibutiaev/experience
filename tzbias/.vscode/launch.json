  "version": "0.2.0",
  "configurations": [
    {
      "name": "executable",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build_Debug/project/executable/tzbias",
      "args": [
      ],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [
        { "name" : "TZ", "value": "Asia/Nicosia" }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}