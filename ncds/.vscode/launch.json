  "version": "0.2.0",
  "configurations": [
    {
      "name": "domtest",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build_Debug/nasdaq/dom/test/domtest",
      "args": [
        "--log_level=all"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [
        {
          "name": "LD_LIBRARY_PATH",
          "value": "/opt/librdkafka-2.6.0.dbg/lib"
        }
      ],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
