{
  "configurations": [
    {
      "name": "J-link Lounch Keyboard",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}/build_Debug/keyboard.elf",
      "request": "launch",
      "type": "cortex-arm-learning",
      "device": "STM32F105RC",
      "interface": "swd",
      "servertype": "jlink",
      "runToMain": true,
    },
    {
      "name": "J-link Attach Keyboard",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}/build_Debug/keyboard.elf",
      "request": "attach",
      "type": "cortex-arm-learning",
      "device": "STM32F105RC",
      "interface": "swd",
      "servertype": "jlink",
      "runToMain": false,
    },
    {
      "name": "st-link Lounch Keyboard",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}/build_Debug/keyboard.elf",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "stutil",
      "interface": "swd",
      "runToMain": false,
      "device": "STM32F107RC",
      "xxpreLaunchCommands": ["Build all"],
    },
    {
      "name": "st-link Attach Keyboard",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}/build_Debug/keyboard.elf",
      "request": "attach",
      "type": "cortex-debug",
      "servertype": "stutil",
      "interface": "swd",
      "runToMain": false,
      "device": "STM32F107RC",
      "xxpreLaunchCommands": ["Build all"],
    }
  ]
}